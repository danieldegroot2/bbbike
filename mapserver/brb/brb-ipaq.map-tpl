    END
    SYMBOL "circle"
    SIZE 3
    COLOR [% COLOR_LIGHT_BLUE %]
  END
  CLASS
    EXPRESSION "W2"
    LABEL
      MINFEATURESIZE AUTO
      MINDISTANCE 50
      POSITION auto
      TYPE truetype
      FONT "lucida"
      MINSIZE 7
      COLOR 0 0 255
      ANGLE AUTO
    END
    SYMBOL "circle"
    SIZE 6
    COLOR [% COLOR_LIGHT_BLUE %]
  END
END

