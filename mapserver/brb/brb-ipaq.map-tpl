LAYER
  NAME "Straﬂen"
  TYPE LINE
  STATUS [% ON_STRASSEN %]
  DATA strassen
  GROUP strassen
  
  LABELITEM "NAME"

  CLASSITEM 'CAT'
  CLASS
    EXPRESSION 'NN'
    NAME "Nebenstraﬂe, f¸r Kfz gesperrt"
    LABEL
      MINFEATURESIZE AUTO
      MINDISTANCE 50
      POSITION auto
      TYPE truetype
      FONT "street"
      MINSIZE 5
      COLOR 0 0 0
#      OUTLINECOLOR [% COLOR_GREY_BG %]
      ANGLE AUTO
    END
    COLOR [% COLOR_GREEN %]
    SYMBOL 'width'
    SIZE 1
  END
  CLASS
    EXPRESSION 'N'
    NAME "Nebenstraﬂe"
    LABEL
      MINFEATURESIZE AUTO
      MINDISTANCE 50
      POSITION auto
      TYPE truetype
      FONT "street"
      MINSIZE 5
      COLOR 0 0 0
#      OUTLINECOLOR [% COLOR_GREY_BG %]
      ANGLE AUTO
    END
    COLOR [% COLOR_WHITE %]
    SYMBOL 'width'
    SIZE 1
  END
  CLASS
    EXPRESSION 'H'
    NAME "Hauptstraﬂe"
    LABEL
      MINFEATURESIZE AUTO
      MINDISTANCE 50
      POSITION auto
      TYPE truetype
      FONT "street"
      MINSIZE 6
      COLOR 0 0 0
#      OUTLINECOLOR [% COLOR_GREY_BG %]
      ANGLE AUTO
    END
    COLOR [% COLOR_YELLOW %]
    SYMBOL 'width'
    SIZE 2
  END
  CLASS
    EXPRESSION 'HH'
    NAME "Wichtige Hauptstraﬂe"
    LABEL
      MINFEATURESIZE AUTO
      MINDISTANCE 50
      POSITION auto
      TYPE truetype
      FONT "street"
      MINSIZE 6
      COLOR 0 0 0
#      OUTLINECOLOR [% COLOR_GREY_BG %]
      ANGLE AUTO
    END
    COLOR [% COLOR_YELLOW %]
    SYMBOL 'width'
    SIZE 2
  END
  CLASS
    EXPRESSION 'B'
    NAME "Bundesstraﬂe"
    LABEL
      MINFEATURESIZE AUTO
      MINDISTANCE 50
      POSITION auto
      TYPE truetype
      FONT "street"
      MINSIZE 6
      COLOR 0 0 0
#      OUTLINECOLOR [% COLOR_GREY_BG %]
      ANGLE AUTO
    END
    COLOR [% COLOR_RED %]
    SYMBOL 'width'
    SIZE 2
  END
  CLASS # should not happen
    EXPRESSION /./ 
    COLOR 200 200 200
  END
END

